<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="STDERR" class="ch.qos.logback.core.ConsoleAppender">
    <target>System.err</target>
    <encoder>
      <pattern>[%-5level]: %msg%n%nopex</pattern>
    </encoder>
  </appender>

  <root level="WARN">
    <appender-ref ref="STDERR" />
  </root>

</configuration>
